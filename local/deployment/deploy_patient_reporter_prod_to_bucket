#!/usr/bin/env bash

source message_functions || exit 1

version=7.24
patient_reporter_jar=${HOME}/hmf/repos/hmftools/patient-reporter/target/patient-reporter-${version}-jar-with-dependencies.jar

if [ ! -f ${patient_reporter_jar} ]; then
    error "Could not locate patient reporter jar!"
fi

info "Copying ${patient_reporter_jar} to gs://common-tools/"
gsutil cp ${patient_reporter_jar} gs://common-tools/patientreporter/${version}/patient-reporter.jar

