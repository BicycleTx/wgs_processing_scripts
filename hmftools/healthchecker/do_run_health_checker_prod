#!/bin/bash

prefix=`dirname $(readlink $0 || echo $0)`

rundir=$1

exec java -Xmx8G -Xms4G \
    -jar /data/common/tools/healthchecker_v2.2/health-checker.jar \
    -report_file_path ${rundir}/logs/HealthCheck.json \
    -run_dir ${rundir} \
    > ${rundir}/logs/HealthCheck.out

echo "HealthChecker Done"