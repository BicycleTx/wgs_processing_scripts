#!/bin/bash

fields=BASELINE.CARCINOMA.CARCINOMA.PTUMLOC,BIOPSY.BIOPS.BIOPSIES.BILESSITE,BIOPSY.BIOPS.BIOPSIES.BILESTYPE,BIOPSY.BIOPS.BIOPSIES.BILESLOC

exec java -Xmx3200m -Xms800m -jar /data/common/tools/ecrfanalyser_v1.0/ecrf-analyser.jar -csvout ~/tmp/ecrf_dump_for_patients.csv -patientasrow -ecrf /data/ecrf/cpct_ecrf.xml -fields ${fields} -patients "$@"