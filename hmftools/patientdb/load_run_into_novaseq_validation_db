#!/bin/bash
source load_metadata

run_dir=$1 && shift
patient_db_jar=/data/common/tools/patient-db_v3.15/patient-db.jar
credentials=/data/common/dbs/mysql_credentials/novaseq_validation
sample=$(load_tumor_sample_from_metadata ${run_dir})

load_genomic_data_for_run_into_db ${run_dir} ${patient_db_jar} ${credentials} ${sample}