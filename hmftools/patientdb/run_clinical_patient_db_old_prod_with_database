#!/usr/bin/env bash

source locate_files || exit 1

credentials=$(locate_old_prod_database_credentials)
db_user=$(awk '/^user/ {print $2}' ${credentials})
db_pass=$(awk '/^pass/ {print $2}' ${credentials})
db_url=$(awk '/^url/ {print $2}' ${credentials})

run_clinical_patient_db_old_prod -do_load_clinical_data -db_user ${db_user} -db_pass ${db_pass} -db_url ${db_url} "$@"