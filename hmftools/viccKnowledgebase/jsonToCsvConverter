#!/usr/bin/env bash

vicc_knowledgebase_importer_jar=/data/experiments/knowledgebase_vicckb/vicc-knowledgebase-importer.jar

knowledgebasePath=/data/experiments/knowledgebase_vicckb
allJson=all.json
brcaJson=brca.json
cgiJson=cgi.json
civicJson=civic.json
jaxJson=jax.json
jaxTrialsJson=jax_trials.json
molecularMatchJson=molecularmatch.json
molecularMatchTrialsJson=molecularmatch_trials.json
oncokbJson=oncokb.json
pmkbJson=pmkb.json
sageJson=sage.json

java -jar ${vicc_knowledgebase_importer_jar} \
    -allFile ${allJson} \
    -brcaFile ${brcaJson} \
    -cgiFile ${cgiJson} \
    -civicFile ${civicJson} \
    -jaxFile ${jaxJson} \
    -jaxTrialsFile ${jaxTrialsJson} \
    -molecularMatchFile ${molecularMatchJson} \
    -molecularMatchTrialsFile ${molecularMatchTrialsJson} \
    -oncokbFile ${oncokbJson} \
    -pmkbFile ${pmkbJson} \
    -sageFile ${sageJson}