#!/bin/sh

patientreport_mailer_dir=/data/common/tools/patientreportmailer_v1.0

email_template=$patientreport_mailer_dir/template.txt
center_csv=$patientreport_mailer_dir/centers.csv

java -jar /data/common/tools/patientreportmailer_v1.0/report-mailer.jar -center_csv "$center_csv" -template "$email_template" "$@"
