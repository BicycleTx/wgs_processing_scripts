#!/usr/bin/env bash

source message_functions || exit 1

dataFile=$1 && shift

iclusionFile="iclusion_trials_prod.tsv"
iclusionPath="/data/resources/iclusion/"
iclusionFileDate=$( echo ${dataFile}_${iclusionFile})

info "Uploading iClusion file to gs://hmf-crunch-resources/serve/iclusion/"
gsutil cp ${iclusionPath}${iclusionFile} gs://hmf-crunch-resources/serve/iclusion/${iclusionFileDate}
info "iClusion has been uploaded in bucket!"
