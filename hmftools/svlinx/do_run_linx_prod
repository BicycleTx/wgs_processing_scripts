#!/bin/bash
source locate_files

prefix=`dirname $(readlink $0 || echo $0)`
run_dir=$1 && shift
output_dir=${run_dir}/sv/

credentials=$(locate_prod_database_credentials)
linx_jar=$(locate_prod_linx)

drup_tsg_file=/data/common/dbs/drup/drup_tsg_genes.csv

${prefix}/do_run_linx ${run_dir} ${credentials} ${linx_jar} ${output_dir} -drup_tsg_file ${drup_tsg_file} $@
