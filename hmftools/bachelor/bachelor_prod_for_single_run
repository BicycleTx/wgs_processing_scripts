#!/usr/bin/env bash

prefix=`dirname $(readlink $0 || echo $0)`

run_dir=$1

# KODU: Doesnt use prod releases yet, pending validation.
credentials=/data/common/dbs/mysql_credentials/prod
bachelor_jar=/data/common/tools/bachelor_pilot/bachelor.jar
bachelor_pp_jar=/data/common/tools/bachelor_pp_pilot/bachelor_pp.jar

${prefix}/do_run_bachelor ${run_dir} ${credentials} ${bachelor_jar} ${bachelor_pp_jar}