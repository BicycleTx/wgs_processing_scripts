#!/usr/bin/env bash

tumor_sample=$1 && shift

if [[ -z "${tumor_sample}" ]]; then
    echo "[ERROR] No tumor sample provided. Exiting" && exit 1;
fi

comments="This is a pilot patient report and is not meant for production purpose."

pilot_do_run_qc_fail_patient_reporter ${tumor_sample} "low_dna_yield" -comments "${comments}" "$@"
