#!/bin/bash

source locate_files

prefix=`dirname $(readlink $0 || echo $0)`
shallow_run=$1 && shift
${prefix}/pilot_do_run_base_patient_reporter \
    -not_analysable \
    -not_analysable_reason low_tumor_percentage \
    -not_analysable_sample \
    -run_directory_shallow ${shallow_run} \
    -purity \
     "$@"