#!/usr/bin/env bash

source locate_files || exit 1

id_generator_jar=$(locate_pilot_hmf_id_generator)
database="hmfpatients_pilot"
sample_hashes_in=/data/resources/custom/idgenerator/hashes/sample_hashes.csv
sample_hashes_out=/data/resources/custom/idgenerator/hashes/new_pilot_sample_hashes.csv

do_run_amber_hmf_id_generator ${id_generator_jar} ${database} ${sample_hashes_in} ${sample_hashes_out}