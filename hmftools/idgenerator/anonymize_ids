#!/bin/bash

basic_id_generator_script=/data/common/repos/scripts/hmftools/idgenerator/do_run_hmf_id_generator
password=$1
sample_ids_file=$2
patient_mapping_file=$3

${basic_id_generator_script} \
    -anonymize_ids \
    -password ${password} \
    -sample_ids_file ${sample_ids_file} \
    -patient_mapping_file ${patient_mapping_file}

