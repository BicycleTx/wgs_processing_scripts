#!/bin/bash

source message_functions || exit 1

version=$1 && shift

if [[ -z "${version}" ]]; then
    error "Version missing. Exiting.";
fi

curl --oauth2-bearer "$(gcloud auth print-access-token)"  -o /data/actin/system/actin.jar -L \
  https://europe-west4-maven.pkg.dev/actin-build/build-registry-maven/com/hmf/actin/system/${version}/system-${version}.jar
